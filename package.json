{
  "name": "url-shortener",
  "type": "module",
  "version": "0.0.1",
  "scripts": {
    "dev": " concurrently \"npm run emulator:firestore\" \"astro dev\"",
    "start": "astro dev",
    "build": "astro check && astro build",
    "preview": "astro preview",
    "astro": "astro",
    "lint": " eslint --fix \"src/**/*.{js,jsx,ts,tsx,md,mdx,astro}\"",
    "lint:style": "stylelint \"src/**/*.{css,scss,astro}\" --fix",
    "clean": "rimraf  ./.vite ./.vercel",
    "format": "prettier --write  \"**/*.{js,jsx,ts,tsx,md,mdx,astro}\" --config prettier.config.mjs",
    "emulator:firestore": "firebase emulators:start --export-on-exit=./src/mocks --import=./src/mocks --only firestore"
  },
  "dependencies": {
    "@astrojs/check": "^0.4.1",
    "@astrojs/node": "^8.0.0",
    "@astrojs/react": "^3.0.9",
    "@astrojs/tailwind": "^5.1.0",
    "@astrojs/vercel": "^7.0.2",
    "@astrojs/vue": "^4.0.8",
    "@types/react": "^18.2.48",
    "@types/react-dom": "^18.2.18",
    "@vercel/analytics": "^1.1.2",
    "@vercel/speed-insights": "^1.0.8",
    "astro": "^4.2.1",
    "firebase": "^10.8.1",
    "firebase-admin": "^12.0.0",
    "qrcode": "^1.5.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "short-unique-id": "^5.0.3",
    "tailwindcss": "^3.4.1",
    "typescript": "^5.3.3",
    "vue": "^3.4.15"
  },
  "devDependencies": {
    "@types/qrcode": "^1.5.5",
    "@typescript-eslint/parser": "^6.19.1",
    "concurrently": "^8.2.2",
    "eslint": "^8.56.0",
    "eslint-plugin-astro": "^0.31.3",
    "eslint-plugin-prettier": "^5.1.3",
    "husky": "^9.0.6",
    "prettier": "^3.2.4",
    "prettier-plugin-astro": "^0.13.0",
    "rimraf": "^5.0.5",
    "stylelint": "^16.2.0",
    "stylelint-config-standard": "^36.0.0"
  }
}
